%% Golang definition for listings
%% http://github.io/julienc91/lstlistings-golang
%%
%% LR: removed this list of tokens that are not keywords:
%%    morekeywords=[5]{Println,Printf,Error,Print,},%

\RequirePackage{listings}

\lstdefinelanguage{Golang}%
  {morekeywords=[1]{break,case,chan,const,continue,default,defer,else,%
     fallthrough,for,func,go,goto,if,import,interface,map,package,range,%
     return,select,struct,switch,type,var},%
   morekeywords=[2]{string,uint,uint8,uint16,uint32,uint64,int,int8,int16,%
     int32,int64,bool,float32,float64,complex64,complex128,byte,rune,uintptr,%
     error,interface},%
   morekeywords=[3]{panic,recover,iota},%
   morekeywords=[4]{map,slice,make,new,nil,len,cap,copy,close,true,false,%
     delete,append,real,imag,complex,chan},%
   sensitive=true,%
   morecomment=[l]{//},%
   morecomment=[s]{/*}{*/},%
   morestring=[b]',%
   morestring=[b]",%
   morestring=[s]{`}{`},%
   }
